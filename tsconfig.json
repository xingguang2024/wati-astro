{
  "extends": "astro/tsconfigs/strict",
  "include": ["./src/env.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strictNullChecks": true,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
