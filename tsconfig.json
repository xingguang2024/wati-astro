{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "./src/env.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "src/components/starwind"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "strictNullChecks": true,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
